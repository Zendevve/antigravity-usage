{
  "name": "antigravity-usage",
  "displayName": "Antigravity Usage",
  "publisher": "zendevve",
  "repository": {
    "type": "git",
    "url": "https://github.com/Zendevve/antigravity-usage.git"
  },
  "description": "Premium AI usage dashboard for AntiGravity with burn rate tracking, exhaustion predictions, and beautiful insights",
  "license": "MIT",
  "version": "0.4.0",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other",
    "Visualization"
  ],
  "keywords": [
    "antigravity",
    "quota",
    "ai",
    "usage",
    "dashboard",
    "analytics"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "extensionKind": [
    "ui"
  ],
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "antigravity-usage-container",
          "title": "Antigravity Usage",
          "icon": "resources/dashboard.svg"
        }
      ]
    },
    "views": {
      "antigravity-usage-container": [
        {
          "id": "antigravity-usage.quotaView",
          "name": "Quota",
          "type": "webview",
          "icon": "resources/dashboard.svg"
        }
      ]
    },
    "commands": [
      {
        "command": "antigravity-quota.showDashboard",
        "title": "Antigravity: Open Dashboard",
        "icon": "$(dashboard)"
      },
      {
        "command": "antigravity-quota.showMenu",
        "title": "Antigravity: Quick Status"
      },
      {
        "command": "antigravity-quota.refresh",
        "title": "Antigravity: Refresh Quota"
      },
      {
        "command": "antigravity-quota.detectPort",
        "title": "Antigravity: Re-detect Port"
      },
      {
        "command": "antigravity-quota.pinModel",
        "title": "Antigravity: Pin Model..."
      },
      {
        "command": "antigravity-quota.unpinModel",
        "title": "Antigravity: Unpin Model..."
      },
      {
        "command": "antigravity-quota.cleanCache",
        "title": "Antigravity: Clean Cache",
        "icon": "$(trash)"
      },
      {
        "command": "antigravity-quota.manageCache",
        "title": "Antigravity: Manage Brain Tasks..."
      }
    ],
    "configuration": {
      "title": "Antigravity Usage",
      "properties": {
        "antigravity.warningThreshold": {
          "type": "number",
          "default": 25,
          "minimum": 5,
          "maximum": 50,
          "description": "Show warning notification when quota drops below this percentage"
        },
        "antigravity.refreshInterval": {
          "type": "number",
          "default": 60,
          "minimum": 30,
          "maximum": 300,
          "description": "How often to refresh quota data (in seconds)"
        },
        "antigravity.pinnedModels": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "Model names to pin (e.g. 'Claude Sonnet', 'Gemini Pro'). Pinned models appear first in status bar and sidebar."
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "package": "npx vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  },
  "dependencies": {
    "axios": "^1.6.0"
  }
}
